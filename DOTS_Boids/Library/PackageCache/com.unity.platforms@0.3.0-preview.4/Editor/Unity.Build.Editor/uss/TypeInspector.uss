.TypeInspector__helpbox
{
    flex-direction: row;
    align-items: center;
    border-width: 1px;
    border-radius: 5px;
}

.TypeInspector__icon
{
    flex-shrink: 0;
    width: 24px;
    height: 24px;
    margin: 5px;
}

.TypeInspector__message
{
    white-space: normal;
    flex-shrink: 1;
    margin: 5px 5px 5px 0px;
}
